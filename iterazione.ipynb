    "# col ciclo 'while'\n",
    "somma = 0   # accumulatore\n",
    "i = 0   # contatore\n",
    "while i < 5:\n",
    "    n = int(input(\"Inserisci un numero: \"))\n",
    "    somma += n   # somma = somma + n\n",
    "    i += 1    # i = i + 1\n",
    "print(\"La somma dei 5 numeri è:\", somma)"
